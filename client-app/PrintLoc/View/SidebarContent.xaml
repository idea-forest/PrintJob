<UserControl x:Class="PrintLoc.View.SidebarContent"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PrintLoc.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Margin="20">
        <TextBlock Text="Dashboard" Foreground="White" FontWeight="Bold" FontSize="24" Margin="0 0 0 20"/>
        <Button x:Name="PrintJobsButton" Content="Print Jobs" Margin="0 0 0 10" Click="PrintJobsButton_Click"/>
        <Button x:Name="ReportsButton" Content="Reports" Margin="0 0 0 10" Click="ReportsButton_Click"/>
        <Button x:Name="PrinterListButton" Content="Printer List" Margin="0 0 0 10" Click="PrinterListButton_Click"/>
    </StackPanel>
</UserControl>
